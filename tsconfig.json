  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "Bundler",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "declaration": true
  },
  "include": ["ts/**/*.ts"],
  "exclude": ["ts/**/*.test.ts", "ts/**/*.bench.ts"]
}
